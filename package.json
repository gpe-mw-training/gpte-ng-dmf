    "core-js": "2.5.1",
    "font-awesome": "4.7.0",
    "intl": "1.2.5",
    "ionicons": "2.0.1",
    "jquery": "^3.3.1",
    "leaflet": "1.2.0",
    "nebular-icons": "1.0.6",
    "node-sass": "~4.8.1",
    "normalize.css": "6.0.0",
    "pace-js": "1.0.2",
